pipeline {
    agent {
        label 'node2'
    }
    stages {
        stage("CHECK ELEMENTS") {
            steps {
                script {
                    def mvnPom = readMavenPom file: 'pom.xml'
                    echo "VERSION: ${mvnPom.version}"
                    echo "ARTIFACT: ${mvnPom.artifactId}"
                }
            }
        }
    }
    post {
        always {
            cleanWs ()
        }
    }
}
