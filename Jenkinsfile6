pipeline {
  agent none
  stages {
    stage("MVN BUILD") {
      agent {label 'docker-node'}
      steps {
        sh 'mvn package'
        echo ""
        echo ">>CHECK FILES<<"
        sh 'ls target/'
        sh 'pwd'
      }
    }
    stage("REMOVE IMAGE") {
      agent { label 'node2'}
      steps {
        sh 'docker rmi devopsdock1/mvn-docker'
      }
    }
  }
}
